{
  "manifest_version": 2,
  "name": " JiraHub",
  "version": "0.1",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*"
      ],
      "js": [
        "client.bundle.js"
      ]
    }
  ],
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "https://*/"
  ]
}
